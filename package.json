{
    "name": "everdev",
    "displayName": "EverDev",
    "description": "Everscale Dev Environemnt",
    "publisher": "TONLabs",
    "version": "1.1.1",
    "author": "TON DEV SOLUTIONS LTD.",
    "license": "Apache-2.0",
    "licenses": [
        {
            "type": "Apache-2.0",
            "url": "http://www.apache.org/licenses/LICENSE-2.0"
        }
    ],
    "icon": "images/tondev.png",
    "engines": {
        "vscode": "^1.52.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:everdev.clang.create",
        "onCommand:everdev.clang.compile",
        "onCommand:everdev.clang.version",
        "onCommand:everdev.clang.set",
        "onCommand:everdev.clang.update",
        "onCommand:everdev.sol.create",
        "onCommand:everdev.sol.compile",
        "onCommand:everdev.sol.ast",
        "onCommand:everdev.sol.version",
        "onCommand:everdev.sol.set",
        "onCommand:everdev.sol.update",
        "onCommand:everdev.se.info",
        "onCommand:everdev.se.version",
        "onCommand:everdev.se.update",
        "onCommand:everdev.se.set",
        "onCommand:everdev.se.start",
        "onCommand:everdev.se.stop",
        "onCommand:everdev.se.reset",
        "onCommand:everdev.se.delete",
        "onCommand:everdev.network.add",
        "onCommand:everdev.network.delete",
        "onCommand:everdev.network.list",
        "onCommand:everdev.network.info",
        "onCommand:everdev.network.default",
        "onCommand:everdev.network.giver",
        "onCommand:everdev.signer.generate",
        "onCommand:everdev.signer.add",
        "onCommand:everdev.signer.delete",
        "onCommand:everdev.signer.list",
        "onCommand:everdev.signer.info",
        "onCommand:everdev.signer.default",
        "onCommand:everdev.contract.info",
        "onCommand:everdev.contract.topup",
        "onCommand:everdev.contract.deploy",
        "onCommand:everdev.contract.run",
        "onCommand:everdev.contract.run-local",
        "onCommand:everdev.contract.run-executor",
        "onCommand:everdev.js.create",
        "onCommand:everdev.js.demo",
        "onCommand:everdev.js.wrap",
        "onCommand:everdev.tonos-cli.install",
        "onCommand:everdev.tonos-cli.set",
        "onCommand:everdev.tonos-cli.version",
        "onCommand:everdev.tonos-cli.update",
        "onCommand:everdev.ts4.version",
        "onCommand:everdev.ts4.install",
        "onCommand:everdev.ts4.update",
        "onCommand:everdev.ts4.create",
        "onCommand:everdev.ts4.run",
        "onCommand:everdev.debrowser.version",
        "onCommand:everdev.debrowser.start",
        "onCommand:everdev.debrowser.stop",
        "onCommand:everdev.debrowser.interfaces"
    ],
    "main": "./out/extension.js",
    "repository": {
        "url": "https://github.com/tonlabs/everdev-vscode"
    },
    "contributes": {
        "commands": [
            {
                "command": "everdev.clang.create",
                "category": "everdev-clang",
                "title": "Create C++ Contract"
            },
            {
                "command": "everdev.clang.compile",
                "category": "everdev-clang",
                "title": "Compile C++ Contract"
            },
            {
                "command": "everdev.clang.version",
                "category": "everdev-clang",
                "title": "Show C++ Compiler Version"
            },
            {
                "command": "everdev.clang.set",
                "category": "everdev-clang",
                "title": "Change C++ Compiler Config"
            },
            {
                "command": "everdev.clang.update",
                "category": "everdev-clang",
                "title": "Update C++ Compiler"
            },
            {
                "command": "everdev.sol.create",
                "category": "everdev-sol",
                "title": "Create Solidity Contract"
            },
            {
                "command": "everdev.sol.compile",
                "category": "everdev-sol",
                "title": "Compile Solidity Contract"
            },
            {
                "command": "everdev.sol.ast",
                "category": "everdev-sol",
                "title": "AST of all source files in a JSON or compact-JSON format."
            },
            {
                "command": "everdev.sol.version",
                "category": "everdev-sol",
                "title": "Show Solidity Version"
            },
            {
                "command": "everdev.sol.set",
                "category": "everdev-sol",
                "title": "Change Solidity Config"
            },
            {
                "command": "everdev.sol.update",
                "category": "everdev-sol",
                "title": "Update Solidity Compiler"
            },
            {
                "command": "everdev.se.info",
                "category": "everdev-se",
                "title": "Show SE Info"
            },
            {
                "command": "everdev.se.version",
                "category": "everdev-se",
                "title": "Show SE Versions"
            },
            {
                "command": "everdev.se.update",
                "category": "everdev-se",
                "title": "Update SE Instance Version"
            },
            {
                "command": "everdev.se.set",
                "category": "everdev-se",
                "title": "Update SE Instance Config"
            },
            {
                "command": "everdev.se.start",
                "category": "everdev-se",
                "title": "Start SE Instance"
            },
            {
                "command": "everdev.se.stop",
                "category": "everdev-se",
                "title": "Stop SE Instance"
            },
            {
                "command": "everdev.se.reset",
                "category": "everdev-se",
                "title": "Reset SE Instance"
            },
            {
                "command": "everdev.se.delete",
                "category": "everdev-se",
                "title": "Delete SE from list"
            },
            {
                "command": "everdev.network.add",
                "category": "everdev-network",
                "title": "Add net"
            },
            {
                "command": "everdev.network.delete",
                "category": "everdev-network",
                "title": "Delete network from registry"
            },
            {
                "command": "everdev.network.list",
                "category": "everdev-network",
                "title": "Prints list of networks"
            },
            {
                "command": "everdev.network.info",
                "category": "everdev-network",
                "title": "Prints network detailed information"
            },
            {
                "command": "everdev.network.default",
                "category": "everdev-network",
                "title": "Set default network"
            },
            {
                "command": "everdev.network.giver",
                "category": "everdev-network",
                "title": "Set giver for network"
            },
            {
                "command": "everdev.signer.generate",
                "category": "everdev-signer",
                "title": "Add signer with randomly generated keys"
            },
            {
                "command": "everdev.signer.add",
                "category": "everdev-signer",
                "title": "Add signer"
            },
            {
                "command": "everdev.signer.delete",
                "category": "everdev-signer",
                "title": "Delete signer from registry"
            },
            {
                "command": "everdev.signer.list",
                "category": "everdev-signer",
                "title": "Prints list of registered signers"
            },
            {
                "command": "everdev.signer.info",
                "category": "everdev-signer",
                "title": "Get signer detailed information"
            },
            {
                "command": "everdev.signer.default",
                "category": "everdev-signer",
                "title": "Set default signer"
            },
            {
                "command": "everdev.contract.info",
                "category": "everdev-contract",
                "title": "Prints contract summary"
            },
            {
                "command": "everdev.contract.topup",
                "category": "everdev-contract",
                "title": "Top up account from giver"
            },
            {
                "command": "everdev.contract.deploy",
                "category": "everdev-contract",
                "title": "Deploy contract to network"
            },
            {
                "command": "everdev.contract.run",
                "category": "everdev-contract",
                "title": "Run contract deployed on the network"
            },
            {
                "command": "everdev.contract.run-local",
                "category": "everdev-contract",
                "title": "Run contract locally on TVM"
            },
            {
                "command": "everdev.contract.run-executor",
                "category": "everdev-contract",
                "title": "Emulate transaction executor locally on TVM"
            },
            {
                "command": "everdev.js.create",
                "category": "everdev-js",
                "title": "Create FreeTON JS App"
            },
            {
                "command": "everdev.js.demo",
                "category": "everdev-js",
                "title": "Download FreeTON Demo"
            },
            {
                "command": "everdev.js.wrap",
                "category": "everdev-js",
                "title": "Wrap ABI file into JavaScript code."
            },
            {
                "command": "everdev.tonos-cli.install",
                "category": "everdev-tonos-cli",
                "title": "Install latest stable TON OS CLI"
            },
            {
                "command": "everdev.tonos-cli.set",
                "category": "everdev-tonos-cli",
                "title": "Change installed version"
            },
            {
                "command": "everdev.tonos-cli.version",
                "category": "everdev-tonos-cli",
                "title": "Show installed and available versions"
            },
            {
                "command": "everdev.tonos-cli.update",
                "category": "everdev-tonos-cli",
                "title": "Update to the latest version"
            },
            {
                "command": "everdev.ts4.version",
                "category": "everdev-ts4",
                "title": "Show installed and available versions"
            },
            {
                "command": "everdev.ts4.install",
                "category": "everdev-ts4",
                "title": "Install a specific release of TestSuite4"
            },
            {
                "command": "everdev.ts4.update",
                "category": "everdev-ts4",
                "title": "Update to the latest version"
            },
            {
                "command": "everdev.ts4.create",
                "category": "everdev-ts4",
                "title": "Create TestSuite4 test"
            },
            {
                "command": "everdev.ts4.run",
                "category": "everdev-ts4",
                "title": "Run TestSuite4's test"
            },
            {
                "command": "everdev.debrowser.version",
                "category": "everdev-debrowser",
                "title": "Show DeBrowser Versions"
            },
            {
                "command": "everdev.debrowser.start",
                "category": "everdev-debrowser",
                "title": "Start ExtraTON DeBrowser, default is 'latest'"
            },
            {
                "command": "everdev.debrowser.stop",
                "category": "everdev-debrowser",
                "title": "Stop ExtraTON DeBrowser"
            },
            {
                "command": "everdev.debrowser.interfaces",
                "category": "everdev-debrowser",
                "title": "Show list of implemented interfaces"
            }
        ],
        "menus": {
            "explorer/context": [
                {
                    "when": "explorerResourceIsFolder",
                    "command": "everdev.clang.create",
                    "group": "EverDev"
                },
                {
                    "when": "resourceFilename =~ /\\.cpp$/i",
                    "command": "everdev.clang.compile",
                    "group": "EverDev"
                },
                {
                    "when": "explorerResourceIsFolder",
                    "command": "everdev.sol.create",
                    "group": "EverDev"
                },
                {
                    "when": "resourceFilename =~ /\\.sol$/i",
                    "command": "everdev.sol.compile",
                    "group": "EverDev"
                },
                {
                    "when": "resourceFilename =~ /\\.sol$/i",
                    "command": "everdev.sol.ast",
                    "group": "EverDev"
                },
                {
                    "when": "resourceFilename =~ /\\.abi$|\\.abi\\.json$|\\.tvc$/i",
                    "command": "everdev.contract.info",
                    "group": "EverDev"
                },
                {
                    "when": "resourceFilename =~ /\\.abi$|\\.abi\\.json$|\\.tvc$/i",
                    "command": "everdev.contract.topup",
                    "group": "EverDev"
                },
                {
                    "when": "resourceFilename =~ /\\.abi$|\\.abi\\.json$|\\.tvc$/i",
                    "command": "everdev.contract.deploy",
                    "group": "EverDev"
                },
                {
                    "when": "resourceFilename =~ /\\.abi$|\\.abi\\.json$|\\.tvc$/i",
                    "command": "everdev.contract.run",
                    "group": "EverDev"
                },
                {
                    "when": "resourceFilename =~ /\\.abi$|\\.abi\\.json$|\\.tvc$/i",
                    "command": "everdev.contract.run-local",
                    "group": "EverDev"
                },
                {
                    "when": "resourceFilename =~ /\\.abi$|\\.abi\\.json$|\\.tvc$/i",
                    "command": "everdev.contract.run-executor",
                    "group": "EverDev"
                },
                {
                    "when": "explorerResourceIsFolder",
                    "command": "everdev.js.create",
                    "group": "EverDev"
                },
                {
                    "when": "explorerResourceIsFolder",
                    "command": "everdev.js.demo",
                    "group": "EverDev"
                },
                {
                    "when": "resourceFilename =~ /\\.abi\\.json$/i",
                    "command": "everdev.js.wrap",
                    "group": "EverDev"
                },
                {
                    "when": "explorerResourceIsFolder",
                    "command": "everdev.ts4.create",
                    "group": "EverDev"
                },
                {
                    "when": "resourceFilename =~ /\\.py$/i",
                    "command": "everdev.ts4.run",
                    "group": "EverDev"
                }
            ]
        }
    },
    "scripts": {
        "prep": "node ./out/prepare_package.js",
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "pretest": "npm run compile && npm run lint",
        "lint": "eslint src --ext ts",
        "test": "node ./out/test/runTest.js"
    },
    "dependencies": {
        "everdev": "^1",
        "@tonclient/core": "^1",
        "@tonclient/lib-node": "^1"
    },
    "devDependencies": {
        "@types/glob": "^7.1.3",
        "@types/mocha": "^8.0.4",
        "@types/node": "^12.11.7",
        "@types/vscode": "^1.52.0",
        "@typescript-eslint/eslint-plugin": "^4.9.0",
        "@typescript-eslint/parser": "^4.9.0",
        "eslint": "^7.15.0",
        "glob": "^7.1.6",
        "mocha": "^8.1.3",
        "typescript": "^4.1.2",
        "vscode-test": "^1.4.1"
    }
}